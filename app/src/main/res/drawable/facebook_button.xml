<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Gray shadow. -->
    <item android:left="1dp" android:top="1dp">
        <shape>
            <solid android:color="@color/com_facebook_button_background_color_pressed" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <!-- White foreground. -->
    <item android:bottom="3dp" android:right="2dp">
        <shape>
            <solid android:color="@color/com_facebook_button_background_color" />
            <corners android:radius="4dp" />
        </shape>
    </item>
</layer-list>